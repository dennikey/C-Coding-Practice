#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i, j, k, m;

    printf("Enter an integer : ");
    scanf("%d", &i);
    printf("Enter an integer : ");
    scanf("%d", &j);
    printf("Enter an integer : ");
    scanf("%d", &k);

    m = j >= i && j <= k;

    printf("%d >= %d and %d <= %d yields %d\n", j, i, j, k, m);

    return 0;
}
